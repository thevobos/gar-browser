{
  "expo": {
    "name": "GarBrowser",
    "slug": "gar-browser",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gar.browser"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "7a19f9f8-399f-47a1-a469-0c14c457ffe0"
      }
    },
    "plugins": [
      [
        "expo-secure-store",
        {
          "faceIDPermission": "$(PRODUCT_NAME) Biometrik verilerinize eri≈ümesine izin verin."
        }
      ]
    ]
  }
}
